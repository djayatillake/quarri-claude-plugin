{
  "name": "@quarri/claude-data-tools",
  "version": "1.3.5",
  "description": "Quarri Data Assistant - Natural language data analysis with Quarri",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "quarri-mcp": "./dist/index.js",
    "quarri-auth": "./dist/auth-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "auth": "node dist/auth-cli.js",
    "auth:status": "node dist/auth-cli.js status",
    "auth:request": "node dist/auth-cli.js request",
    "auth:verify": "node dist/auth-cli.js verify",
    "test:mcp": "./test-mcp.sh",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "quarri",
    "mcp",
    "claude",
    "data-analysis",
    "sql",
    "natural-language"
  ],
  "author": "Quarri AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/djayatillake/quarri-claude-plugin"
  },
  "dependencies": {
    "@mcp-ui/server": "^6.0.0",
    "@modelcontextprotocol/ext-apps": "^1.0.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    ".claude-plugin",
    "skills"
  ]
